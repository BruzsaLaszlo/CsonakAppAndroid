<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CsorloFragment">

    <Button
        android:id="@+id/button_to_led"
        android:layout_width="123dp"
        android:layout_height="47dp"
        android:layout_marginEnd="292dp"
        android:layout_marginBottom="4dp"
        android:enabled="true"
        android:text="@string/next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/buttonElore"
        android:layout_width="147dp"
        android:layout_height="54dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="32dp"
        android:text="@string/elore"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonHatra"
        android:layout_width="147dp"
        android:layout_height="50dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="36dp"
        android:text="@string/hatra"
        app:layout_constraintStart_toEndOf="@+id/buttonElore"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonStop"
        android:layout_width="360dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="108dp"
        android:text="@string/stop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/textMultiLine"
        android:layout_width="364dp"
        android:layout_height="377dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="31dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonStop" />

    <Button
        android:id="@+id/buttonMaps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="119dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/button_to_led"
        app:layout_constraintTop_toBottomOf="@+id/textMultiLine" />

</androidx.constraintlayout.widget.ConstraintLayout>